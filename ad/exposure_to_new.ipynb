    "import pandas as pd \n",
    "df = pd.read_csv(\"exposure_to_new.csv\")\n",
    "\n",
    "from sklearn.linear_model import LinearRegression\n",
    "\n",
    "x = df[[\"exposure\"]]\n",
    "y = df[[\"new_user\"]]\n",
    "\n",
    "lr = LinearRegression()\n",
    "lr.fit(x,y)\n",
    "\n",
    "y_predict = lr.predict([[200000],[250000],[300000]])\n",
    "\n",
    "print(y_predict)"
   ]
  }
